{
  "expo": {
    "name": "Hamstash",
    "slug": "hamstash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hamLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/hamLogo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.stephen.hamstash",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/hamLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/images/hamLogo.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-sqlite"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bd88dd8d-c15b-4e3d-aec3-de9fd6ed3016"
      }
    }
  }
}
